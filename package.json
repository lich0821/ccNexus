{
  "name": "ccnexus",
  "version": "1.2.0",
  "description": "Claude Code 智能端点轮换代理",
  "type": "module",
  "scripts": {
    "start": "node run.mjs",
    "dev": "node run.mjs",
    "build": "node run.mjs -b",
    "build:prod": "node run.mjs -b -p",
    "build:windows": "node run.mjs -b --platform windows/amd64",
    "build:macos": "node run.mjs -b --platform darwin/universal",
    "build:linux": "node run.mjs -b --platform linux/amd64",
    "install:frontend": "cd frontend && npm install",
    "lint:frontend": "cd frontend && npm run lint",
    "clean": "rm -rf build/bin frontend/dist frontend/node_modules"
